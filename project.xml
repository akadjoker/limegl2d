<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="BunnyMark" package="org.openfl.lime.demos.bunnymark" version="1.0.0" company="OpenFL" />
	<app main="Main" path="Export" file="BunnyMark" />
	
	<window fps="61" />
	<window width="800" height="600" if="html5" />
	
	<source path="Source" />
	
	<!-- <define name="webgl" if="html5" /> -->
	<define name="stats" if="html5" />
	
	<haxelib name="lime" />
	
	
	<assets path="assets/graphics" rename="gfx" include="*.png|*.jpg" />
	<assets path="assets/audio" rename="sfx" include="*.wav|*.ogg" />
	<assets path="assets/atlas" rename="atlas" include="*.xml|*.tmx|*.pex|*.fnt|*.txt" />

	
</project>